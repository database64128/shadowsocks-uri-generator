{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch ss-uri-gen in Interactive Mode",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build (Debug)",
            "program": "${workspaceFolder}/ShadowsocksUriGenerator/bin/Debug/net9.0/ss-uri-gen.dll",
            "args": [
                "interactive"
            ],
            "cwd": "${workspaceFolder}/ShadowsocksUriGenerator/bin/Debug/net9.0",
            "stopAtEntry": false,
            "console": "integratedTerminal"
        },
        {
            "name": ".NET Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
